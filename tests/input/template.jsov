root: 
  display: false
  cascading: vertical
  block-border:
    border-style: solid
    border-width: 2px
    border-color: "#ffffff"
    rounded-corners: 5px
    bgcolor: "#00206a"
    internal-spacing: 8px
  children:
    Players:
      bgcolor: "#00bbbb"
      fgcolor: "#cccccc"
      rounded-corners: 5px
      title:
        bgcolor: "#ffffff"
        fgcolor: "#000000"
        rounded-corners: 5px
        link: "{parent}/{this}.html"
      default-child:
        fgcolor: "#ffffff"
      children:
        HP:
          bgcolor: "#00dd00"
          link: "{grandparent}/{parent}/{this}"
        MP:
          bgcolor: "#6600dd"
        Bonus:
          bgcolor: "#bbbb00"
        Score:
          bgcolor: "#dd0000"
