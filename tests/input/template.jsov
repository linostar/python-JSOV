root: 
  display: false
  block-border:
    border-style: solid
    border-width: 1px
    border-color: black
  children:
    - Players:
        cascading: vertical
        bgcolor: "#00bbbb"
        fgcolor: "#cccccc"
        rounded-corners: 5px
        title:
          bgcolor: "#ffffff"
          fgcolor: "#000000"
          rounded-corners: 5px
        default-child:
          fgcolor: "#ffffff"
        children:
          - HP:
              bgcolor: "#00dd00"
          - MP:
              bgcolor: "#0000dd"
          - Bonus:
              bgcolor: "#dddd00"
          - Score:
              bgcolor: "#dd0000"
